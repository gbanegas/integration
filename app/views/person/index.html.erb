<h1>Listing Person</h1>
 
<table>
  <tr>
    <th>Name</th>
    <th>Last Name</th>
  </tr>
 
  <% @person.each do |person| %>
    <tr>
      <td><%= person.name %></td>
      <td><%= person.last_name %></td>
      <td><%= link_to 'Show', person_path(person) %></td>
    <td><%= link_to 'Destroy', person_path(person),
                    method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
